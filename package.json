{
  "name": "connected-next-router",
  "version": "2.0.0-alpha.4",
  "description": "A Redux binding for Next.js Router",
  "author": "Daniel Reinoso <danielreinoso1807@gmail.com> (http://danielr18.github.io)",
  "license": "MIT",
  "keywords": [
    "next",
    "next.js",
    "next-router",
    "next-router-redux",
    "connected-react-router",
    "react-router-redux"
  ],
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "tsc --outDir dist",
    "build:es": "tsc --module esnext --outDir dist/es",
    "watch": "cross-env BABEL_ENV=commonjs babel src -d . -w --extensions \".ts,.tsx\"",
    "clean": "git clean -xdfe node_modules",
    "type-check": "tsc --noEmit",
    "lint": "npm run type-check && eslint './src/**/*.{ts,tsx}'"
  },
  "peerDependencies": {
    "next": "^9.2.0",
    "react": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^3.6.0 || ^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^13.1.8",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.6",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "next": "^9.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danielr18/connected-next-router.git"
  },
  "bugs": {
    "url": "https://github.com/danielr18/connected-next-router/issues"
  }
}
